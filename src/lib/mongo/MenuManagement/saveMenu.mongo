 function insertMenuData(args) {
 if (!args._id) {
 args._id=new ObjectId()
 args.OUId=ObjectId(args.OUid)
     db.menu.insert(args)
     console.log('insert',args)
     }
 else{
 args._id = ObjectId(args._id)
 db.menu.update({_id: args._id}, {$set: args})
