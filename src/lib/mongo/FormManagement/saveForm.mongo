function saveForm(args) {
    var mydate = new Date();
    var dates = ( mydate.getFullYear() + "/" + (mydate.getMonth() + 1) + "/" + mydate.getDate() + " " +
    mydate.getHours() + ":" + mydate.getMinutes() + ":" + mydate.getSeconds())
    if (!args._id) {
        args.createTime = dates;
        args.lastmodifyTime = dates;
        db.form.insert(args)
    }
    else {
        args.lastmodifyTime = dates;
        db.form.update(args)
    }
}



